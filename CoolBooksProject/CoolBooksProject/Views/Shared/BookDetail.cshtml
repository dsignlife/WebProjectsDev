@model CoolBooksProject.Models.Books

<div class="thumbnail">
    <img alt="@Model.Title" src="@Model.ImagePath">
    <div class="caption-full">
        <h3 class="pull-right">@Model.GenreId</h3>
        <h3>
            <a href="#">@Model.Title</a>
        </h3>

        <p>@Model.Description</p>

    </div>
</div>